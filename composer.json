{
    "name": "archy_bold/laravel-music-services",
    "description": ":A Laravel package for adding service-agnostic music functionality.",
    "license": "license",
    "authors": [
        {
            "name": "Simon Archer",
            "email": "hello@archybold.com",
            "homepage": "https://archybold.com"
        }
    ],
    "homepage": "https://github.com/archy-bold/laravel-music-services",
    "keywords": [
        "Laravel",
        "laravel-music-services",
        "spotify",
        "apple-music",
        "deezer",
        "napster"
    ],
    "require": {
        "illuminate/database": "~5|~6|~7",
        "illuminate/support": "~5|~6|~7",
        "jwilsson/spotify-web-api-php": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0",
        "timacdonald/log-fake": "^1.6",
        "php-mock/php-mock-phpunit": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "ArchyBold\\LaravelMusicServices\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArchyBold\\LaravelMusicServices\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArchyBold\\LaravelMusicServices\\MusicServicesServiceProvider"
            ],
            "aliases": {
                "LaravelMusicServices": "ArchyBold\\LaravelMusicServices\\Facades\\LaravelMusicServices"
            }
        }
    }
}
